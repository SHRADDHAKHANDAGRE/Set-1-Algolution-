#include <iostream>
using namespace std;

void Pattern(int rows)
{
    for (int i = 0; i < rows; i++) 
    {
        
        for (int j = 1; j <= rows; j++)
        {
            
            if (j <= i) 
            {
                cout << (i + 1);  
            } 
            else
            {
                cout << (j);      
            }
        }
        cout << endl; 
    }
}

int main()
{
    int rows;

    cout << "Enter number of rows: ";
    cin >> rows;

    Pattern(rows);

    return 0;
}
